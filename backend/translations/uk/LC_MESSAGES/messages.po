msgid "Confirmed"
msgstr "Підтверджено"

#: templates/layout.html:96
msgid "Refuted"
msgstr "Спростовано"

#: templates/layout.html:97
msgid "Misleading"
msgstr "Оманливо"

#: templates/layout.html:98
msgid "Partly True"
msgstr "Частково правдиво"

#: templates/layout.html:99
msgid "Unverifiable"
msgstr "Неможливо перевірити"

#: templates/layout.html:100
msgid "Claim text missing"
msgstr "Текст твердження відсутній"

#: templates/layout.html:101
msgid "No verdict"
